msgstr "Ar norite ištrinti šį aplanką ?"

msgid "Note: This directory is not empty - it contains %s item(s)."
msgstr "Pastaba: Šis aplankas nėra tuščias - jame yra %s elementas(i)."

msgid ""
"You have changed the path to the picon folder.\n"
"Accept changed path ?"
msgstr ""
"Pakeitėte kelią į „picon“ aplanką.\n"
"Sutikti pakeistą kelią ?"

msgid ""
"New plugin version found: %s\n"
"Do you want to install it now ?"
msgstr ""
"Rasta nauja papildinio versija: %s\n"
"Ar norite ją įdiegti dabar ?"

